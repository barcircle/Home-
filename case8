#include <iostream>

using namespace std;

int main(){
    int d,m;
    cin>>d>>m;
    d -=1;
    switch (d){
        case 0:{switch (m-1){
                case 1:
                case 3:
                case 5:
                case 7:
                case 8:
                case 10:
                case 12:
                    cout<<"31 "<<m-1;
                    break;
                case 2:
                    cout<<"29 "<<m-1;
                    break;
                case 4:
                case 6:
                case 9:
                case 11:
                    cout<<"30 "<<m-1;
                    break;
                default:
                    cout<<"\n Nope";
            }}
        default: cout<<d<<" "<<m;
    }


    return 0;
}
