#include <iostream>

using namespace std;

int main(){
    int N;
    double a;
    cin>>N>>a;
    switch (N){
        case 1:{cout<<a/10; break; }
        case 2:{cout<<a*1000; break;}
        case 3:{cout<<a; break;}
        case 4:{cout<<a/1000; break;}
        case 5:{cout<<a/100; break;}
        default: cout<<"\nnope";
    }


    return 0;
}
