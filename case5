#include <iostream>

using namespace std;

int main(){
    int N;
    double a, b;
    cin>>N>>a>>b;
    switch (N){
        case 1:
            cout<<a+b;
            break;
        case 2:
            cout<<a-b;
            break;
        case 3:
            cout<<a*b;
            break;
        case 4:
            cout<<a/b;
            break;
        default:
            cout<<"\nnope";
    }


    return 0;
}
