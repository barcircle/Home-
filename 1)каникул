/*Напишите программу, которая удаляет из массива 
все простые числа встречающиеся больше одного раза*/


#include <iostream>

using namespace std;

bool prost(int n){
    if (n <= 1)
        return false;
    else if (n == 2)
        return true;
    for (int i = 2; i<n; i++){
        if (n%i == 0){
            return false;
        }
    }
    return true;
}

void povtor(int *m, int size) {
    for (int i = 0; i < size; i++)
    {
        for (int j = i + 1; j < size ; j++)
        {
            if ( m[i] == m[j] && prost(m[i]) == 1)
            {
                for (int k = j; k < size -1; k++)
                {
                    m[k] = m[k + 1];
                }
                size -= 1;
                if (m[i] == m[j])
                {
                    j--;
                }
            }
        }
    }
    for(int i = 0; i<size;i++){
        cout<<m[i];
    }
}


int main() {
    int a;
    cin >> a;
    int m[a];
    for (int i = 0; i < a; i++)
        cin >> m[i];
    povtor(m,a);
    return 0;
}
