/*Для данного вещественного x найти значение следующей функции f,
принимающей вещественные значения:
f (x) = 2·sin(x), если x > 0,
6 − x, если x ≤ 0.*/

#include <iostream>
#include <cmath>

using namespace std;

int main(){
    cout<<"Введите число :";
    double val;
    cin>>val;

    if (val > 0){
        cout<<2*sin(val);
    }
    else
        cout<<6-val;
    return 0;
}
